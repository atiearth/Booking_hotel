package com.cp.project.demo.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class WebController {
	
	@GetMapping("/")
	public String getHomePage() {
		return "homepage";
	}
	
	@GetMapping("/web/logout")
	public String afterLogout() {
		return "homepage";
	}

	@GetMapping("/homepage_login")
	public String getHomePageLogin() {
		return "homepage_login";
	}
	
	@GetMapping("/web/view/booking")
	public String getViewBooking() {
		return "viewbooking";
	}
	
	@GetMapping("/web/view/dashboard")
	public String getViewDashboard() {
		return "dashboard";
	}	

}
